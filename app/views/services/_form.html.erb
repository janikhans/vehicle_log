<%= simple_form_for(@service) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
  </div>

  <%= f.simple_fields_for :fields do |builder| %>
    <%= render 'field_fields', f: builder %>
  <% end %>
  <div class="links">
    <%= link_to_add_association "Add Field", f, :fields %>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
